{
  "version": "2.0",
  
  "extensions": {
    "http": {
        "routePrefix": ""
      }
    },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {          
       "default": "Trace"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}